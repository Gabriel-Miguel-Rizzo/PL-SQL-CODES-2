DECLARE

  TYPE Numero_Table_Type IS VARRAY (10) OF INTEGER(2);
  Numero_table numero_table_type := numero_table_type();
  
BEGIN
 
  FOR i IN 1..10 LOOP
  
    Numero_Table.extend;
    Numero_Table(i) := i;
    
  END LOOP;
 
 
  FOR i IN 1..10 LOOP
  
    DBMS_OUTPUT.PUT_LINE('Varray: Indice = ' || TO_CHAR(i) || ', Valor = ' || TO_CHAR(Numero_Table(i)));
  END LOOP;
END;
